{
  "expo": {
    "name": "mobile-mieszkaniownik",
    "slug": "mobile-mieszkaniownik",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mobilemieszkaniownik",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.mobilemieszkaniownik"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "softwareKeyboardLayoutMode": "resize",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.mobilemieszkaniownik",
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-camera"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
