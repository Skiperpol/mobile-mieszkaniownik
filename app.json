{
  "expo": {
    "name": "mobile-mieszkaniownik",
    "slug": "mobile-mieszkaniownik",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mobilemieszkaniownik",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.mobilemieszkaniownik",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Aplikacja potrzebuje dostępu do galerii, aby dodać zdjęcia do ogłoszeń.",
        "NSPhotoLibraryAddUsageDescription": "Aplikacja potrzebuje dostępu do galerii, aby zapisać zdjęcia."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "softwareKeyboardLayoutMode": "resize",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.mobilemieszkaniownik",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES"
      ]
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-camera",
      [
        "expo-image-picker",
        {
          "photosPermission": "Aplikacja potrzebuje dostępu do galerii, aby dodać zdjęcia do ogłoszeń."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
